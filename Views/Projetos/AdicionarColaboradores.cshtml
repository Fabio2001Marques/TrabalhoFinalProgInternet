@model TrabalhoFinalProgInternet.Models.Projeto


@{
    ViewData["Title"] = "AdicionarColaboradores";
}

<h1>Adicionar Colaboradores</h1>

<div class="form-group">
    <label asp-for="ProjetoColaboradores" class="control-label"></label>
    <select asp-for="ProjetoColaboradores" class="form-control" asp-items="ViewBag.ColaboradorId"></select>
</div>
<div class="form-group">
    <input type="submit" value="Adicionar" class="btn btn-primary" />
    <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
</div>

